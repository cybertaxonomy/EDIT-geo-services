          </HTTP>
        </DCPType>
      </GetCapabilities>
      <DescribeFeatureType>
        <SchemaDescriptionLanguage>
          <XMLSCHEMA/>
        </SchemaDescriptionLanguage>
        <DCPType>
          <HTTP>
            <Get onlineResource="http://193.190.223.44/ionicweb/wfs/ONE_GEOLOGY_RMCA?"/>
            <Post onlineResource="http://193.190.223.44/ionicweb/wfs/ONE_GEOLOGY_RMCA"/>
          </HTTP>
        </DCPType>
      </DescribeFeatureType>
      <GetFeature>
        <ResultFormat>
          <GML2/>
        </ResultFormat>
        <DCPType>
          <HTTP>
            <Get onlineResource="http://193.190.223.44/ionicweb/wfs/ONE_GEOLOGY_RMCA?"/>
